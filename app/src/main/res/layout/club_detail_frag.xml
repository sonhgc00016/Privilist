<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="@dimen/common_big_space">
        <com.privilist.component.widget.PrivilistTextView
            android:text="@string/about"
            android:layout_marginTop="@dimen/common_space"
            style="@style/CommonText.Title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.privilist.component.widget.PrivilistTextView
            android:id="@+id/tv_about"
            style="@style/CommonText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.privilist.component.widget.PrivilistTextView
            android:text="@string/hours_of_operation"
            android:layout_marginTop="@dimen/common_space"
            style="@style/CommonText.Title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <!--<com.privilist.component.widget.PrivilistTextView-->
            <!--android:id="@+id/tv_hours"-->
            <!--style="@style/CommonText"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="wrap_content"/>-->
        <LinearLayout
            android:id="@+id/hour_oprerationLlt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/common_space"
            android:layout_marginTop="@dimen/common_space"
            android:orientation="vertical">

        </LinearLayout>
        <com.privilist.component.widget.PrivilistTextView
            android:text="@string/dress_code"
            android:layout_marginTop="@dimen/common_space"
            style="@style/CommonText.Title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.privilist.component.widget.PrivilistTextView
            android:id="@+id/tv_dress_code"
            style="@style/CommonText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.privilist.component.widget.PrivilistTextView
            android:text="@string/fine_print"
            android:layout_marginTop="@dimen/common_space"
            style="@style/CommonText.Title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <!--<TableLayout
            android:id="@+id/table_fines"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        </TableLayout>-->
        <com.privilist.component.widget.PrivilistTextView
            android:id="@+id/tv_fine_content"
            style="@style/CommonText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="50dp"
            />
    </LinearLayout>
</ScrollView>